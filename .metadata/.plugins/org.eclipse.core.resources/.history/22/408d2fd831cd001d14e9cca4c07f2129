package quiz;

public class QuizEx01 {
	public static void main(String[] args) {
		
		int pay = 567890;
		
		System.out.println("만원: " + pay / 10000 +"장");
		System.out.println("천원: " + (pay % 10000) / 1000 +"장");
		System.out.println("백원: " + pay / 10000 +"장");
		System.out.println("십원: " + pay / 10000 +"장");
		
		
	}
}
/*문제1] 월급을 단위로 계산하기
 * int pay = 567890;
 * 
 * result :
 * 만원: 56장
 * 천원:7장
 * 백원:8개
 * 십원:9개
 */
