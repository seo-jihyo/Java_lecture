//*
package ex01.operator;

public class OperatorTest {
	public static void main(String[] args) {
		//shift 연산자 : <<, >>, <<<, >>>
		int x = 8, result;
		
		//00001000 (8)--> 00100000 (32)
		result = x << 2;	//left shift : 원래값 * 2^bit수 --> 8 * 2^2 = 32
		System.out.println("left shift result :" + result); //32
		
	}
}
//*/